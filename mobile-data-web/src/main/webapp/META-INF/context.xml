<?xml version="1.0" encoding="UTF-8"?>
<Context path="/mobile-data-web">
  <!-- INF_VERSION.VERSION -->
  <Environment
    name="Version"
    value="7.8.0"
    type="java.lang.String"
    override="false"/>

  <Environment
    name="Application"
    value="Mobile-Data-Application"
    type="java.lang.String"
    override="false"/>

  <Environment
    name="OracleDriverClassName"
    value="oracle.jdbc.driver.OracleDriver"
    type="java.lang.String"
    override="false"/>

  <Environment
    name="DatabaseServer"
    value="lone219"
    type="java.lang.String"
    override="false"/>

  <Environment
    name="DatabasePort"
    value="1521"
    type="java.lang.String"
    override="false"/>

  <Environment
    name="DatabaseSID"
    value="dev2"
    type="java.lang.String"
    override="false"/>

  <!-- PROJEKT.PRO_ID -->
  <Environment
    name="PRO_ID"
    value="1000"
    type="java.lang.Long"
    override="true"/>

  <!-- PVARIANT.PLV_TYP -->
  <Environment
    name="PLV_TYP"
    value="BEZ"
    type="java.lang.String"
    override="true"/>

<!--  <Environment
    name="Debug"
    value="-debug"
    type="java.lang.String"
    override="false"/>-->

</Context>

